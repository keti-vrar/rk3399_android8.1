-    int OLEDSetPos(int x, int y)
-    { 
-        int ret = 0;
-
-        y = y/8 + y%8;
-        if (OLEDWriteCmd((byte)(0xb0+y)) == -1) {
-            ret = -1;
-        }
-        
-        if (OLEDWriteCmd((byte)((x&0xf0)>>4|0x10)) == -1) {
-            ret = -1;
-        }
-        if (OLEDWriteCmd((byte)((x&0x0f)|0x01)) == -1) {
-            ret = -1;
-        }
-        return ret;
-    } 
